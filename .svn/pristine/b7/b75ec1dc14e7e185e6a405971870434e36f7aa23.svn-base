                                "fn": "onRowEditingEdit",
                                "implHandler": [
                                    "//Actualiza el registro seleccionado con los datos nuevos ingresados ",
                                    "var me = this;",
                                    "var store = Ext.getCmp('grdCasa').getStore();",
                                    "var g = Ext.getCmp('grdCasa');",
                                    "var reco = g.selection;",
                                    "var id = reco.data.id;",
                                    "var arrayKeys = g.getSelectionModel().selected.items;",
                                    "//Si se realizó alguna modificación en los datos del registro seleccionado",
                                    "if (context.record.modified)",
                                    "{",
                                    "    var reco = context.record;",
                                    "    store.add(reco);",
                                    "    store.sync(",
                                    "        {",
                                    "            params:{",
                                    "                id: id,",
                                    "                data: arrayKeys[0]",
                                    "",
                                    "            },",
                                    "            success: function(batch, success)",
                                    "            {",
                                    "                store.commitChanges();",
                                    "                Ext.Msg.alert('Datos Guardados', 'Los datos se han guardado con éxito');",
                                    "                store.removeAll();",
                                    "                store.load();",
                                    "            },",
                                    "",
                                    "            failure: function(batch, success)",
                                    "            {",
                                    "                Ext.Msg.alert('Error', 'Hubor un error');",
                                    "            }",
                                    "        });",
                                    "}"
                                ],
